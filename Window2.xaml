<Window x:Class="health.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:health"
        mc:Ignorable="d"
        Title="Здоровье +" Height="450" Width="800">
    <Grid Background="#FF278B27">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="175"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid Background="#FF278B27" >
            <StackPanel>
                <Image Source="Logo.png" Margin="10"/>
                <Button Width="150" Height="40" Margin="10,10,10,10" Click="ShowPatients">Пациенты</Button>
                <Button Width="150" Height="40" Margin="10,0,10,10" Click="ShowWorkers">Врачи</Button>
                <Button Width="150" Height="40" Margin="10,0,10,10" Click="ShowProcedure">Услуги</Button>
               
            </StackPanel>
        </Grid>
        <Frame Grid.Column="1" x:Name="frame1"/>

    </Grid>
</Window>
